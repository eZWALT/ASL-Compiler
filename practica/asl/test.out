function extrafunc
  params
    a integer array
  endparams

   %1 = 0
   %3 = a
   %2 = %3[%1]
   writei %2
   %4 = 0
   %5 = a
   %6 = 5
   %5[%4] = %6
   %7 = 0
   %9 = a
   %8 = %9[%7]
   writei %8
   return
   return
endfunction

function main
  vars
    a integer 2
  endvars

   %1 = 0
   %2 = 0
   a[%1] = %2
   %3 = 1
   %4 = 1
   a[%3] = %4
   %5 = &a
   pushparam %5
   call extrafunc
   popparam 
   %6 = 0
   %7 = a[%6]
   writei %7
   %8 = 1
   %9 = a[%8]
   writei %9
   return
   return
endfunction


